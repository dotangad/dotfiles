[user]
	name = Angad Singh
	email = mail@angad.dev
	signingkey = BA87FB758E3920E9
[gpg]
	program = /usr/bin/gpg2
[commit]
	gpgsign = true
[alias]
	co = checkout
	c = commit -m
	caa = commit -am
	aa = add --all
	s = status
	br = branch
	hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
	type = cat-file -t
	dump = cat-file -p
	l = log --oneline
	last = log -1
	lg = log --graph --abbrev-commit --decorate --format=format:'%C(bold blue)%h%C(reset) - %C(bold green)(%ar)%C(reset) %C(white)%s%C(reset) %C(dim white)- %an%C(reset)%C(bold yellow)%d%C(reset)' --all
[credential]
	helper = osxkeychain
[color]
	ui = auto
